{
  "use": [
    "postcss-cssnext",
    "postcss-import",
    "postcss-sorting"
  ],
  "input": "src/css/core.css",
  "output": "docs/css/bundle.css",
  "autoprefixer": {
    "browsers": [
      "ie >= 11",
      "last 2 Edge versions",
      "last 2 Firefox versions",
      "last 2 Chrome versions",
      "last 2 Safari versions",
      "last 2 Opera versions",
      "last 2 iOS versions",
      "last 2 ChromeAndroid versions"
    ]
  }
}

